{
  "name": "person-service-rust",
  "version": "0.1.0",
  "bin": {
    "person-service-rust": "bin/person-service-rust.js"
  },
  "scripts": {
    "build": "tsc",
    "build:fns": "cargo lambda build --release --arm64",
    "build:fns:dev": "cargo lambda build --arm64",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "dev": "pnpm build:fns:dev && pnpm cdk deploy --watch --require-approval=never",
    "deploy": "pnpm build:fns && pnpm cdk deploy",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "22.5.5",
    "aws-cdk": "2.158.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "4.19.1",
    "typescript": "~5.6.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.158.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "zod": "3.23.8"
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts,json,md}": "prettier --write",
    "*.rs": "cargo fmt --all --"
  },
  "packageManager": "pnpm@8.7.4+sha1.9dfcac89c4010a00ce092c3cda76dab72cd99198"
}
